{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "jsdoc-format": [
        false
    ],
    "rules": {
        "trailing-comma": [
            false
        ],
        "interface-over-type-literal": false,
        "max-line-length": [
            false
        ],
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ],
        "typedef": [
            true,
            "call-signature",
            "property-declaration"
        ],
        "indent": [
            true,
            "tabs",
            4
        ]
    },
    "rulesDirectory": []
}